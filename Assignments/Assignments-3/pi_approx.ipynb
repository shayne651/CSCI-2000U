def rho_n(num):
    sum=0
    for x in range(1,num+1):
        sum+=((-1)**(x+1))/((2*x)-1)
    print ('When n = ',num,' rho_n = ',4*sum)
def tau_n(num):
    sum=0
    for x in range(1,num+1):
        sum+=(1/(x**2))
    print ('when n = ',num,' tau_n=',(6*sum)**(1/2))
def mu_n(num):
    sum=0
    for x in range(1,num+1):
        sum+=1/x**4
    print('when n = ',num,' mu_n =  ',(90*sum)**(1/4))

rho_n(10)
tau_n(10)
mu_n(10)
